{
  "name": "antd-doddle",
  "version": "2.0.2",
  "license": "ISC",
  "description": "antd后台项目前端组件封装和方法库",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development bisheng start",
    "presite": "rm -rf ./guide/README.md && cp -rf README.md ./guide",
    "site": "cross-env NODE_ENV=production bisheng build",
    "build": "webpack --config webpack.config.js",
    "lib": "gulp lib",
    "es": "gulp es",
    "propub": "gulp && webpack --config webpack.config.js",
    "prepublish": "gulp",
    "postpublish": "rm -rf es && rm -rf lib"
  },
  "files": [
    "es",
    "dist",
    "lib",
    "utils",
    "decorator",
    "hooks"
  ],
  "notes": "备注：下面的设置是针对于正式环境tree-shaking时，css会被当做无副作用清理掉",
  "sideEffects": [
    "./packages/*/index.less",
    "./es/*/index.less",
    "./lib/*/index.less"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/closertb/antd-doddle.git"
  },
  "keywords": [],
  "author": "denzelt",
  "dependencies": {
    "@wry/equality": "^0.1.9",
    "dayjs": "^1.8.28",
    "memoize-one": "^5.1.1",
    "react-transition-group": "^4.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.0",
    "@babel/plugin-proposal-decorators": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-classes": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.5",
    "@doddle/doddle-bisheng-theme": "^1.0.3",
    "@doddle/eslint-config-doddle": "^0.0.11",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "antd": "^4.4.0",
    "antd-dayjs-webpack-plugin": "^1.0.0",
    "babel-loader": "^8.0.5",
    "bind-decorator": "^1.0.11",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.9",
    "del": "^3.0.0",
    "dva": "^2.4.1",
    "file-loader": "^2.0.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-typescript": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "invariant": "^2.2.4",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "merge2": "^1.2.3",
    "mini-css-extract-plugin": "^0.4.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.20.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "url-loader": "^0.6.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  }
}
